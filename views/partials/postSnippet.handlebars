<!doctype html>

<html>

	<div class="panel {{postCategory}} postSnippet" data-cat="{{postCategory}}" data-title="{{postTitle}}" data-info="{{postInfo}}" data-full="{{fullPost}}" data-bump="{{bumpCount}}">
	  <div class="panel-heading">
		<h3 class="panel-title">
			{{postTitle}}
			<div class="pull-right">
				{{bumpCount}} 
			</div>
		</h3>
		  </div>
	  <div class="panel-body">
	    {{postInfo}}
	     <br><a id="expandPost" href="/">Expand</a>
	  </div>
	</div>

</html>
